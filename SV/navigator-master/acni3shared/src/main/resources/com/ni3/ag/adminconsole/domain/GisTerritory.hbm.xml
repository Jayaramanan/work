<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ni3.ag.adminconsole.domain.GisTerritory" table="gis_territory">
		<id name="id">
			<column name="id" />
			<generator class="identity" />
		</id>

		<property name="territory" column="territory" length="50" />
		<property name="label" column="label" length="50" />
		<property name="sort" column="sort" />
		<property name="tableName" column="tableName" />
		<property name="displayColumn" column="displayColumn" />
		<property name="version" column="version" />

	</class>
</hibernate-mapping>