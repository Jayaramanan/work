<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ni3.ag.adminconsole.domain.Icon" table="cht_icons">
		<id name="id" column="ID">
			<generator class="identity" />
		</id>

		<property name="iconName" column="IconName" length="100" not-null="true"/>
		<property name="icon" column="Icon" type="binary" />
	</class>
</hibernate-mapping>