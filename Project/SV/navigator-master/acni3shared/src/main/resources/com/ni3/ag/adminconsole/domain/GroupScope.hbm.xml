<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ni3.ag.adminconsole.domain.GroupScope" table="sys_group_scope" lazy="false">
		<id name="group" column="GroupID">
			<generator class="assigned"/>
		</id>
		<property name="nodeScope" column="nodeScope" type="text"/>
		<property name="edgeScope" column="edgeScope" type="text"/>
	</class>
</hibernate-mapping>